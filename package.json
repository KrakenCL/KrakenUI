{
  "name": "KrakenUI",
  "version": "0.0.1",
  "description": "A lightweight JS UI components for KrakenCL",
  "main": "dist/kraken-ui.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js --mode development --inline --hot",
    "build": "webpack --config build/webpack.ui.js --optimize-minimize --mode production",
    "clean": "rimraf ./ui/ui.bundle.css ./ui/ui.bundle.js",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty --ext .js,.vue,.html ./",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrakenCL/KrakenUI.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "material design",
    "components",
    "ui"
  ],
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "author": {
    "name": "Volodymyr Pavliukevych"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KrakenCL/KrakenUI/issues"
  },
  "homepage": "https://krakencl.com",
  "dependencies": {
    "extract-text-webpack-plugin": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "autoprefixer": "^9.4.4",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "deepmerge": "^3.0.0",
    "fuzzysearch": "^1.0.3",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "tippy.js": "^3.4.0",
    "vue": "^2.5.21",
    "vue-loader": "^14.2.2",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
